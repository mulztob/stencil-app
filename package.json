{
  "name": "starwarsapi-stencil-testapp",
  "private": true,
  "version": "0.0.1",
  "description": "StarWars Api Test App (swapi.dev)",
  "license": "MIT",
  "type": "module",
  "module": "swapi-stencil",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "e2e": "npx playwright test",
    "e2e:report": "npx playwright show-report",
    "e2e:visible": "npx playwright test --project visible",
    "generate": "stencil generate",
    "postinstall": "patch-package",
    "prerender": "npx stencil build --prerender",
    "preserve": "npm run prerender",
    "serve": "npx http-server ./www --cors"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.26.1",
    "@stencil/core": "2.18.0",
    "@stencil/store": "^2.0.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.186",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "puppeteer": "^18.0.5",
    "stencil-router-v2": "^0.6.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}